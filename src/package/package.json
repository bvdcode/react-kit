{
  "name": "@bvdcode/react-kit",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./assets/*": "./dist/assets/*"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "postbuild": "mkdir -p dist/assets && cp -R src/assets/* dist/assets/"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bvdcode/react-kit"
  },
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.4",
    "@mui/styled-engine": "^7.3.4",
    "react": ">=18",
    "react-dom": ">=18",
    "react-router-dom": ">=7",
    "react-toastify": "^11.0.5",
    "zustand": "^5.0.8",
    "i18next": "^25.6.0",
    "react-i18next": "^16.0.0",
    "i18next-browser-languagedetector": "^8.2.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "axios": "^1.13.2"
  }
}
