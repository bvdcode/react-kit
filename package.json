{
  "name": "@bvdcode/react-kit",
  "version": "0.1.0",
  "description": "React application toolkit: pages-driven routing, layout, auth gating; foundation for future UI, state and data modules.",
  "keywords": [
    "react",
    "router",
    "pages",
    "kit"
  ],
  "author": "bvdcode",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bvdcode/react-kit.git"
  },
  "bugs": {
    "url": "https://github.com/bvdcode/react-kit/issues"
  },
  "homepage": "https://github.com/bvdcode/react-kit#readme",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --minify --clean",
    "dev": "tsup src/index.ts --dts --format cjs,esm --watch",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "react-router-dom": ">=6"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "tsup": "^8.2.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  }
}
