{
  "name": "@bvdcode/react-kit",
  "version": "0.1.0",
  "description": "React application toolkit: pages-driven routing, layout, auth gating; foundation for future UI, state and data modules.",
  "keywords": [
    "react",
    "router",
    "pages",
    "kit"
  ],
  "author": "bvdcode",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bvdcode/react-kit.git"
  },
  "bugs": {
    "url": "https://github.com/bvdcode/react-kit/issues"
  },
  "homepage": "https://github.com/bvdcode/react-kit#readme",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --minify --clean",
    "dev": "tsup src/index.ts --dts --format cjs,esm --watch",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@mui/types": "^7.4.8",
    "@mui/x-charts": "^8.17.0",
    "@mui/x-date-pickers": "^8.17.0",
    "@mui/x-data-grid": "^8.17.0"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "tsup": "^8.2.4",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5",
  }
}
